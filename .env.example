# ClearMe API Configuration
# Copy this file to .env and fill in your actual API keys

# Grok (Groq) API Keys - Get from https://console.groq.com/keys
# These are tried first in order. If one fails, the next is tried.
GROK_API_KEY_PRIMARY=your_grok_api_key_here
GROK_API_KEY_2=your_second_grok_key_here
GROK_API_KEY_3=your_third_grok_key_here
GROK_API_KEY_4=your_fourth_grok_key_here

# OpenAI API Key - Get from https://platform.openai.com/api-keys
# This is used as fallback if all Grok keys fail
OPENAI_API_KEY=your_openai_api_key_here

# App Configuration
APP_NAME=ClearMe
APP_VERSION=1.0.0
